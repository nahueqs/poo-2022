                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_4acEoHBPEe2OSfFA2RChjA" name="recibirIntentoJugada" specification="_4Z-xoHBPEe2OSfFA2RChjA">
                    <ownedParameter xmi:id="_4acEoXBPEe2OSfFA2RChjA" name="carta" type="_WN8_4G_QEe2JNZU2l2g9fw"/>
                    <ownedParameter xmi:id="_4acEonBPEe2OSfFA2RChjA" type="_WOLpYG_QEe2JNZU2l2g9fw" direction="return">
                      <name xsi:nil="true"/>
                    </ownedParameter>
                    <language>java</language>
                    <body>// la jugada valida es que sea menor a la carta que esta en la mesa, o que sea del mismo color.&#xD;
		if (carta.getNumero() >= getUltimaCarta().getNumero()) { // es mayor al numero en mesa&#xD;
			if (carta.getColor().equals(getUltimaCarta().getColor())){ // es igual al color de la mesa&#xD;
				return Eventos.JUGADA_CORRECTA;&#xD;
			} else {&#xD;
				return Eventos.JUGADA_INCORRECTA;&#xD;
			} 		&#xD;
		}&#xD;
		return Eventos.JUGADA_CORRECTA;&#xD;
	</body>
                  </ownedBehavior>
