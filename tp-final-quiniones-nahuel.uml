                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_uHuscHBUEe2OSfFA2RChjA" name="iniciarJuego" specification="_uHSnkHBUEe2OSfFA2RChjA">
                    <language>java</language>
                    <body>estado = Eventos.JUEGO_INICIADO;&#xD;
		siguienteTurno = jugadores.get(0);&#xD;
		llenarPila();&#xD;
		mezclarPila();&#xD;
		repartir();&#xD;
		this.notificar(estado);&#xD;
	</body>
                  </ownedBehavior>
                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_0lzhIHBqEe2OSfFA2RChjA" name="llenarPila" specification="_0lk3oHBqEe2OSfFA2RChjA">
                    <language>java</language>
                    <body>for (Colores color : Colores.values()) {&#xD;
			for(int j = 1;j&lt;=10;j++) {&#xD;
				pila.push(new Carta (j, color));&#xD;
			}&#xD;
		}&#xD;
	</body>
                  </ownedBehavior>
                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_T7mfUHBrEe2OSfFA2RChjA" name="mezclarPila" specification="_T7cHQHBrEe2OSfFA2RChjA">
                    <language>java</language>
                    <body>int size = pila.size();&#xD;
		Random rand = new Random();&#xD;
		List&lt;Carta> a = new ArrayList&lt;Carta>();&#xD;
		for (int i = 1; i &lt;= size;i++ ) {&#xD;
			a.add(pila.pop());&#xD;
		}&#xD;
		for (int i = size - 1 ; i > 0; i--) {&#xD;
			int numRandom = rand.nextInt(i+1);&#xD;
			Carta temp = a.get(numRandom);&#xD;
			a.set(numRandom, a.get(i));&#xD;
			a.set(i, temp);&#xD;
		}		&#xD;
		for (int i = 0; i &lt; size; i++) {&#xD;
			pila.push(a.get(i));&#xD;
		}								&#xD;
	</body>
                  </ownedBehavior>
                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_OoU88HBtEe2OSfFA2RChjA" name="setSiguienteTurno" specification="_OoJWwHBtEe2OSfFA2RChjA">
                    <language>java</language>
                    <body>if (jugadorActual.getId() &lt; 4) { &#xD;
			siguienteTurno = jugadores.get(jugadorActual.getId()+1);&#xD;
		} else { &#xD;
			siguienteTurno = jugadores.get(0);&#xD;
		}&#xD;
	</body>
