                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_WStkEG_QEe2JNZU2l2g9fw" name="mezclar" specification="_WOTlMG_QEe2JNZU2l2g9fw">
                    <language>java</language>
                    <body>int size = cartas.size();&#xD;
		Random rand = new Random();&#xD;
		List&lt;Carta> a = new ArrayList&lt;Carta>();&#xD;
		for (int i = 1; i &lt;= size;i++ ) {&#xD;
			a.add(cartas.pop());&#xD;
		}&#xD;
		for (int i = size - 1 ; i > 0; i--) {&#xD;
			int numRandom = rand.nextInt(i+1);&#xD;
			Carta temp = a.get(numRandom);&#xD;
			a.set(numRandom, a.get(i));&#xD;
			a.set(i, temp);&#xD;
		}		&#xD;
		for (int i = 0; i &lt; size; i++) {&#xD;
			cartas.push(a.get(i));&#xD;
		}				&#xD;
		&#xD;
		&#xD;
	</body>
                  </ownedBehavior>
