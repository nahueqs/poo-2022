                      <name xsi:nil="true"/>
                    </ownedParameter>
                    <language>java</language>
                    <body>if (recibirIntentoJugada(carta) == Eventos.JUGADA_CORRECTA) {&#xD;
			cartas.add(carta);&#xD;
			return recibirIntentoJugada(carta);&#xD;
		} else {&#xD;
			return recibirIntentoJugada(carta);&#xD;
		}&#xD;
				&#xD;
	</body>
