                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mORK8HBWEe2OSfFA2RChjA" name="iniciarJuego" specification="_fCLXsHBWEe2OSfFA2RChjA">
                    <language>java</language>
                    <body>boolean salir = false;&#xD;
		List lista = new ArrayList();&#xD;
		while(!salir) {&#xD;
			menuPrincipal();&#xD;
			String opcion = this.entrada.nextLine();&#xD;
			switch (opcion.toLowerCase()) {&#xD;
			case &quot;a&quot;:&#xD;
				menuCargaJugadores();					&#xD;
				break;&#xD;
			case &quot;x&quot;:&#xD;
				salir = true;&#xD;
